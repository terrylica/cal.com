{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/routing-forms",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./emails/components": "./src/emails/components/index.tsx",
    "./components/DynamicAppComponent": "./src/components/DynamicAppComponent.tsx",
    "./components/FormInputFields": "./src/components/FormInputFields.tsx",
    "./components/InfoLostWarningDialog": "./src/components/InfoLostWarningDialog.tsx",
    "./lib/InitialConfig": "./src/lib/InitialConfig.tsx",
    "./lib/processRoute": "./src/lib/processRoute.tsx",
    "./components/_components/EmptyState": "./src/components/_components/EmptyState.tsx",
    "./components/_components/ResultSection": "./src/components/_components/ResultSection.tsx",
    "./components/_components/RoutingSkeleton": "./src/components/_components/RoutingSkeleton.tsx",
    "./components/_components/TeamMemberSelect": "./src/components/_components/TeamMemberSelect.tsx",
    "./components/_components/TeamMembersMatchResult": "./src/components/_components/TeamMembersMatchResult.tsx",
    "./components/react-awesome-query-builder/widgets.test": "./src/components/react-awesome-query-builder/widgets.test.tsx",
    "./components/react-awesome-query-builder/widgets": "./src/components/react-awesome-query-builder/widgets.tsx",
    "./emails/components/ResponseEmail": "./src/emails/components/ResponseEmail.tsx",
    "./emails/components/index": "./src/emails/components/index.tsx",
    "./components/react-awesome-query-builder/config/uiConfig": "./src/components/react-awesome-query-builder/config/uiConfig.tsx",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "emails/components": [
        "./src/emails/components/index.tsx"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "description": "It would allow a booker to connect with the right person or choose the right event, faster. It would work by taking inputs from the booker and using that data to route to the correct booker/event as configured by Cal user  ",
  "dependencies": {
    "@calcom/lib": "workspace:*",
    "dotenv": "16.6.1",
    "json-logic-js": "2.0.2",
    "react-awesome-query-builder": "5.1.2"
  },
  "scripts": {
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*",
    "@types/json-logic-js": "1.2.1"
  }
}
