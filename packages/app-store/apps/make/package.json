{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/make",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./pages/setup/_getServerSideProps": "./src/pages/setup/_getServerSideProps.tsx",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ]
    }
  },
  "description": "Workflow automation for everyone. Use the Cal.com Make app to trigger your workflows when a booking is created, rescheduled, or cancelled, or after a meeting ends.",
  "dependencies": {
    "@calcom/lib": "workspace:*"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*",
    "@types/node-schedule": "2.1.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
