{
  "private": true,
  "name": "@calcom/exchange2013calendar",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/InstallAppButton": "./src/components/InstallAppButton.tsx",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "description": "For calendars hosted on on-premises Microsoft Exchange 2013 servers",
  "dependencies": {
    "@calcom/lib": "workspace:*",
    "@calcom/prisma": "workspace:*",
    "@calcom/ui": "workspace:*",
    "ews-javascript-api": "0.11.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-hook-form": "^7.0.0"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
