{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/btcpayserver",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/EventTypeAppCardInterface": "./src/components/EventTypeAppCardInterface.tsx",
    "./components/EventTypeAppSettingsInterface": "./src/components/EventTypeAppSettingsInterface.tsx",
    "./components/KeyInput": "./src/components/KeyInput.tsx",
    "./pages/setup/_getServerSideProps": "./src/pages/setup/_getServerSideProps.tsx",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "dependencies": {
    "@calcom/lib": "workspace:*"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "description": "BTCPay Server is a self-hosted open source Bitcoin payment processor. Start receiving bitcoin payments for your events and bookings.",
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
