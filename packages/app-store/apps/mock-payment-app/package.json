{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/mock-payment-app",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/EventTypeAppCardInterface": "./src/components/EventTypeAppCardInterface.tsx",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "dependencies": {
    "@calcom/lib": "workspace:*"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "description": "The mock payment app for tests",
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
