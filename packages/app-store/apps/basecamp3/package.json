{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/basecamp3",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/EventTypeAppCardInterface": "./src/components/EventTypeAppCardInterface.tsx",
    "./components/EventTypeAppSettingsInterface": "./src/components/EventTypeAppSettingsInterface.tsx",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "dependencies": {
    "@calcom/lib": "workspace:*"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "description": "Basecamp puts everything you need to get work done in one place. It's the calm, organized way to manage projects, work with clients, and communicate company-wide.",
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
