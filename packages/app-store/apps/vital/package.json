{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/vital",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/AppConfiguration": "./src/components/AppConfiguration.tsx",
    "./components/InstallAppButton": "./src/components/InstallAppButton.tsx",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "description": "Connect your health data or wearables to trigger actions on your calendar.",
  "dependencies": {
    "@calcom/prisma": "workspace:*",
    "@tryvital/vital-node": "1.4.6",
    "queue": "6.0.2"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
