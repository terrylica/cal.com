{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/alby",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/AlbyPriceComponent": "./src/components/AlbyPriceComponent.tsx",
    "./components/EventTypeAppCardInterface": "./src/components/EventTypeAppCardInterface.tsx",
    "./components/EventTypeAppSettingsInterface": "./src/components/EventTypeAppSettingsInterface.tsx",
    "./pages/setup/_getServerSideProps": "./src/pages/setup/_getServerSideProps.tsx",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./src/lib/index.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "dependencies": {
    "@calcom/lib": "workspace:*",
    "@getalby/lightning-tools": "5.1.2",
    "@getalby/sdk": "4.0.0",
    "@webbtc/webln-types": "3.0.0",
    "react-qr-code": "2.0.18",
    "svix": "0.85.1"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  },
  "description": "Your Bitcoin & Nostr companion for the web. Use Alby to charge Satoshi for your Cal.com meetings.\r",
  "scripts": {
    "type-check": "tsc --noEmit"
  }
}
