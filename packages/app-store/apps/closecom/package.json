{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "name": "@calcom/closecom",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./components/EventTypeAppCardInterface": "./src/components/EventTypeAppCardInterface.tsx",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ]
    }
  },
  "description": "Close is the inside sales CRM of choice for startups and SMBs. Make more calls, send more emails and close more deals starting today.",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@calcom/lib": "workspace:*",
    "@calcom/prisma": "workspace:*"
  },
  "devDependencies": {
    "@calcom/types": "workspace:*"
  }
}
