{
  "name": "@calcom/app-store",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "main": "./src/index.ts",
  "exports": {
    "./apps/*": "./apps/*",
    "./templates/*": "./templates/*",
    "./types": "./types.d.ts",
    "./AppDependencyComponent": "./src/AppDependencyComponent.tsx",
    "./BookingPageTagManager": "./src/BookingPageTagManager.tsx",
    "./EventTypeAppContext": "./src/EventTypeAppContext.tsx",
    "./InstallAppButton": "./src/InstallAppButton.tsx",
    "./InstallAppButtonWithoutPlanCheck": "./src/InstallAppButtonWithoutPlanCheck.tsx",
    "./apps.browser.generated": "./src/apps.browser.generated.tsx",
    "./_components/AppCard": "./src/_components/AppCard.tsx",
    "./_components/AppCategoryNavigation": "./src/_components/AppCategoryNavigation.tsx",
    "./_components/AppNotInstalledMessage": "./src/_components/AppNotInstalledMessage.tsx",
    "./_components/AppSettings": "./src/_components/AppSettings.tsx",
    "./_components/DynamicComponent": "./src/_components/DynamicComponent.tsx",
    "./_components/EventTypeAppCardInterface": "./src/_components/EventTypeAppCardInterface.tsx",
    "./_components/EventTypeAppSettingsInterface": "./src/_components/EventTypeAppSettingsInterface.tsx",
    "./_components/OmniInstallAppButton": "./src/_components/OmniInstallAppButton.tsx",
    "./_components/crm/WriteToObjectSettings": "./src/_components/crm/WriteToObjectSettings.tsx",
    "./_pages/setup/_getServerSideProps": "./src/_pages/setup/_getServerSideProps.tsx",
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "apps/*": ["./apps/*"],
      "templates/*": ["./templates/*"],
      "types": ["./types.d.ts"],
      "*": [
        "./src/types/*",
        "./src/*"
      ]
    }
  },
  "files": [
    "components.ts"
  ],
  "scripts": {
    "build:types": "tsc --project tsconfig.build.json",
    "type-check": "tsc --pretty --noEmit",
    "type-check:ci": "tsc-absolute --pretty --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:report": "biome lint --reporter json . > ../../lint-results/app-store.json"
  },
  "dependencies": {
    "@calcom/dailyvideo": "workspace:*",
    "@calcom/dayjs": "workspace:*",
    "@calcom/features": "workspace:*",
    "@calcom/lib": "workspace:*",
    "@calcom/office365video": "workspace:*",
    "@calcom/ui": "workspace:*",
    "@calcom/zoomvideo": "workspace:*",
    "lodash": "4.17.23",
    "qs-stringify": "1.2.1",
    "react-i18next": "12.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "stripe": "^9.0.0 || ^15.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@calcom/testing": "workspace:*",
    "@calcom/types": "workspace:*"
  }
}
